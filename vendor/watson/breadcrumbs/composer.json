{
    "name": "watson/breadcrumbs",
    "description": "Breadcrumbs made easy for Laravel.",
    "keywords": ["laravel", "breadcrumbs"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dwight Watson",
            "email": "dwight@studentservices.com.au"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/routing": "5.*",
        "illuminate/support": "5.*",
        "illuminate/view": "5.*"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Watson\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Watson\\Breadcrumbs\\ServiceProvider"
            ],
            "aliases": {
                "Breadcrumbs": "Watson\\Breadcrumbs\\Facade"
            }
        }
    },
    "minimum-stability": "stable"
}
